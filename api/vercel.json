{
  "version": 2,
  "builds": [
    {
      "src": "**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/food",
      "methods": ["POST"],
      "dest": "/api/createFood.js"
    },
    {
      "src": "/api/food",
      "methods": ["GET"],
      "dest": "/api/getFood.js"
    },
    {
      "src": "/api/food/(.+)",
      "methods": ["PUT"],
      "dest": "/api/updateFood.js"
    },
    {
      "src": "/api/food/(.+)",
      "methods": ["DELETE"],
      "dest": "/api/deleteFood.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "installCommand": "npm install",
    "buildCommand": "echo 'No build step'",
    "outputDirectory": "./"
  }
}