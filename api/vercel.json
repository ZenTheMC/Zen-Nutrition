{
  "version": 2,
  "builds": [
    {
      "src": "**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/$1.js"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "installCommand": "npm install",
    "buildCommand": "echo 'No build step'",
    "outputDirectory": "./"
  }
}
